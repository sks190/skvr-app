                <row><cell>Mistä tien sä tiesit tänne n. 1439</cell><cell>44.</cell></row>
                <row><cell>Toivoisinko taattoni tulevan n. 1440—1446</cell><cell>44—47.</cell></row>
                <row><cell>Tule illall’ igriessäh n. 1447</cell><cell>48.</cell></row>
                <row><cell>Tullah kalevat kaupalta n. 1448</cell><cell>48.</cell></row>
                <row><cell>Tunnen tuomeksen tulennasta n:t 1449—1449 a)</cell><cell>48.</cell></row>
                <row><cell>Viikonko olen villapäänä n:t 1450—1451</cell><cell>49.</cell></row>
                <row><cell>Voi, korppi lintuni, näetkö mun taattoni n. 1452</cell><cell>49.</cell></row>
                <row><cell>TYTTÖJEN KERSKUMISIA:</cell><cell/></row>
                <row><cell>Sivut.</cell><cell/></row>
                <row><cell>Ei täyvy tätä tytärtä pahoille rejen pajuille n. 1453</cell><cell>49—50.</cell></row>
                <row><cell>Käyn kun särkky säärtä lyöden n. 1454</cell><cell>50.</cell></row>
                <row><cell>Kyllä tällä neitosella vielä lempi lentöän tuloo n. 1455</cell><cell>50.</cell></row>
                <row><cell>Oisko meitä kaksi, kolme, järven järkähyttäsimme n. 1456</cell><cell>50.</cell></row>
                <row><cell>Vielä näistä tyttäristä suku juopi suuret sarkat n:t 1457—1458</cell><cell>50.</cell></row>
                <row rend="heading"><cell>PETTYMYS:</cell><cell/></row>
                <row rend="heading"><cell>Sivut.</cell><cell/></row>
                <row><cell>Mikä lie minusta nähty n:t 1459—1462</cell><cell>51.</cell></row>
                <row><cell>Muilla on sulhot muitterammat n. 1463</cell><cell>51.</cell></row>
                <row><cell>On kukka kuihuttoassa n. 1464</cell><cell>51.</cell></row>
                <row><cell>Petettihin pennilöillä n. 1465</cell><cell>51.</cell></row>
                <row rend="heading"><cell>PILKKA:</cell><cell/></row>
                <row rend="heading"><cell>Sivut.</cell><cell/></row>
                <row><cell>Kuin olen joutun’ juomariksi n. 1466</cell><cell>52.</cell></row>
                <row><cell>Jouvuin maille vierahille n. 1467</cell><cell>52.</cell></row>
                <row><cell>Sanottiin sata lasta saaneheni n. 1468</cell><cell>52—53.</cell></row>
                <row><cell>TOIVOMUKSIA:</cell><cell/></row>
                <row><cell>Sivut.</cell><cell/></row>
                <row><cell>Anna, Luoja, luokosäätä n:t 1469—1472</cell><cell>53.</cell></row>
                <row><cell>Anna vastakin, Jumala, viinat virtana vilata n. 1473</cell><cell>53—54.</cell></row>
                <row><cell>Kuku, kultani käköni n. 1474</cell><cell>54.</cell></row>
                <row><cell>Suo vastakin, Jumala, suo sontaista pihoa n. 1475</cell><cell>54.</cell></row>
                <row rend="heading"><cell>VANHEMPAIN TOIVEITA:</cell><cell/></row>
                <row rend="heading"><cell>Sivut.</cell><cell/></row>
                <row><cell>Äsen poika poik’ olisi, kuin leivän lehosta toisi n:t 1476—1478</cell><cell>54—55.</cell></row>
                <row rend="heading"><cell>LAPSEN KIITOLLISUUS:</cell><cell/></row>
                <row rend="heading"><cell>Sivut.</cell><cell/></row>
                <row><cell>Näkisi nämäkin silmät emosen etempeäki n. 1479</cell><cell>55.</cell></row>
                <row rend="heading"><cell>ERILAISIA TUNNELMIA:</cell><cell/></row>
                <row rend="heading"><cell>Sivut.</cell><cell/></row>
                <row><cell>Kesän vuotin kekriäni n:t 1480—1481</cell><cell>55.</cell></row>
                <row><cell>Muoriseni, kun sä nälkäni näkisit n:t 1482—1483</cell><cell>56.</cell></row>
