            <schemaSpec start="TEI teiCorpus" ident="teipublisher" source="tei_simplePrint.odd">
                <elementSpec ident="text" mode="change"><model behaviour="body"/></elementSpec>
                <elementSpec ident="figure" mode="change"><model behaviour="block">
            <outputRendition xml:space="preserve">
            width: 100%; display: block;
            </outputRendition>
        </model><model predicate="head or @rendition='simple:display'" behaviour="block"/><model behaviour="inline"/></elementSpec>
                <elementSpec ident="head" mode="change"><model predicate="@type=&#34;main&#34;" behaviour="heading">
            <param name="level" value="1"/>
            <outputRendition xml:space="preserve">
            font-size: 32px; font-weight: normal !important; margin-bottom: 32px;
            </outputRendition>
        </model><model behaviour="heading">
            <param name="level" value="3"/>
            <outputRendition xml:space="preserve">
            font-family: 'ff-scala-sans';
            </outputRendition>
        </model><model predicate="ancestor::div[@type=&#34;intro&#34;]" behaviour="heading">
            <param name="level" value="3"/>
        </model><model predicate="contains(@xml:id, 'title_2')" behaviour="omit"/><model predicate="contains(@xml:id, 'title_3')" behaviour="omit"/><model predicate="contains(@xml:id, 'notes')" behaviour="block"/><model predicate="$parameters?header='short'" behaviour="inline">
            <param name="content" value="replace(string-join(.//text()[not(parent::ref)]), '^(.*?)[^\w]*$', '$1')"/>
        </model><model predicate="parent::figure" behaviour="block">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model><model predicate="parent::table" behaviour="block">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model><model predicate="parent::lg" behaviour="block">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model><model predicate="parent::list" behaviour="block">
            <outputRendition xml:space="preserve">
            font-weight: bold;
            </outputRendition>
        </model><model predicate="parent::div" behaviour="heading">
            <param name="level" value="count(ancestor::div)"/>
        </model><model behaviour="block"/></elementSpec>
                <elementSpec ident="teiHeader" mode="change"><modelSequence output="web" predicate="$parameters?header='short2'">
            <model behaviour="block">
                <param name="content" value=".//msIdentifier/idno[@type=&#34;nro&#34;]"/>
                <outputRendition xml:space="preserve" scope="before">
                content: 'ID: '; font-weight: bold;
                </outputRendition>
                <outputRendition xml:space="preserve">
                font-size: 16px;
                </outputRendition>
            </model>
            <model behaviour="block">
                <param name="content" value=".//particDesc/person/persName[@type=&#34;standardized&#34;]"/>
                <outputRendition xml:space="preserve" scope="before">
                content: 'Kerääjä: '; font-weight: bold;
                </outputRendition>
                <outputRendition xml:space="preserve">
                font-size: 16px;
                </outputRendition>
            </model>
            <model behaviour="block">
                <param name="content" value=".//setting/placeName/region"/>
                <outputRendition xml:space="preserve" scope="before">
                content: 'Keräysalue (normalisoitu): '; font-weight: bold;
                </outputRendition>
                <outputRendition xml:space="preserve">
                font-size: 16px;
                </outputRendition>
            </model>
            <model behaviour="block">
                <param name="content" value=".//setting/placeName/settlement[@type=&#34;standardized_municipality&#34;]"/>
                <outputRendition xml:space="preserve" scope="before">
                content: 'Keräyspaikka (normalisoitu): '; font-weight: bold;
                </outputRendition>
                <outputRendition xml:space="preserve">
                font-size: 16px;
                </outputRendition>
            </model>
            <model behaviour="block">
                <param name="content" value=".//setting/date[@type=&#34;standardized&#34;]"/>
                <outputRendition xml:space="preserve" scope="before">
                content: 'Keräysvuosi: '; font-weight: bold;
                </outputRendition>
                <outputRendition xml:space="preserve">
                font-size: 16px;
                </outputRendition>
            </model>
        </modelSequence><model output="latex" behaviour="metadata"/><model output="fo" behaviour="omit"/><model output="web" predicate="$parameters?header='short'" behaviour="block"/><model output="web" behaviour="metadata"/></elementSpec>
                <elementSpec ident="fileDesc" mode="change"><modelSequence predicate="$parameters?header='short'">
            <model behaviour="block" cssClass="header-short">
                <param name="content" value="titleStmt"/>
            </model>
        </modelSequence><model behaviour="title">
            <param name="content" value="titleStmt"/>
        </model></elementSpec>
                <elementSpec ident="titleStmt" mode="add"><model output="latex" behaviour="meta">
            <param name="content" value="."/>
        </model><model output="fo" behaviour="heading"/><model predicate="$parameters?mode='title'" behaviour="heading">
            <param name="content" value="title[not(@type)]"/>
            <param name="level" value="5"/>
        </model><modelSequence output="web" predicate="$parameters?header='short'">
            <model behaviour="inline">
                <param name="content" value="let $collection := ./ancestor::teiHeader//collection return if ($collection) then concat('SKVR', ' ', $collection) else 'JR'"/>
            </model>
            <model behaviour="webcomponent">
                <param name="path" value="$parameters?doc"/>
                <param name="content" value="./ancestor::teiHeader//idno[@type=&#34;id&#34;]"/>
                <param name="name" value="'pb-link'"/>
                <param name="emit" value="'poem'"/>
                <outputRendition xml:space="preserve">
                display: inline; width: 50px !important; font-weight: 800 !important; color: #E22717; font-size: 120%;
                </outputRendition>
            </model>
            <model behaviour="inline">
                <param name="content" value="./ancestor::teiHeader//settingDesc//settlement[@type=&#34;standardized_municipality&#34;]"/>
            </model>
            <model behaviour="inline">
                <param name="content" value="./ancestor::teiHeader//profileDesc//persName[@type=&#34;standardized&#34;]"/>
            </model>
            <model behaviour="inline">
                <param name="content" value="./ancestor::teiHeader//idno[@type=&#34;sgn&#34;]"/>
                <outputRendition xml:space="preserve">
                margin-right: 4rem !important;
                </outputRendition>
            </model>
            <model behaviour="inline">
                <param name="content" value="./ancestor::teiHeader//profileDesc/settingDesc//date[@type=&#34;standardized&#34;]"/>
                <outputRendition xml:space="preserve">
                display: inline;
                </outputRendition>
            </model>
        </modelSequence><model output="web" behaviour="block"/></elementSpec>
                <elementSpec ident="publicationStmt" mode="add"><model output="web" behaviour="block">
            <param name="content" value="availability/licence"/>
        </model><model behaviour="omit"/></elementSpec>
                <elementSpec ident="licence" mode="add"><model output="web" predicate="@target" behaviour="link" cssClass="licence">
            <param name="content" value="'Licence'"/>
            <param name="uri" value="@target"/>
        </model><model behaviour="omit"/></elementSpec>
                <elementSpec ident="title" mode="change"><model predicate="$parameters?header='short'" behaviour="heading">
            <param name="level" value="4"/>
            <param name="content" value="let $id := ./ancestor::teiHeader//idno[@type=&#34;id&#34;] return if (contains($id, '.')) then concat('SKVR ',./ancestor::teiHeader//collection, ' ', ./ancestor::teiHeader//idno[@type=&#34;id&#34;]) else concat('JR ',./ancestor::teiHeader//collection, ' ', ./ancestor::teiHeader//idno[@type=&#34;id&#34;])"/>
            <outputRendition xml:space="preserve" scope="before">
            
            </outputRendition>
        </model><model predicate="$parameters?header='short2'" behaviour="inline">
            <param name="level" value="4"/>
            <param name="content" value="concat(./ancestor::teiHeader//setting//settlement[@type=&#34;non_standardized&#34;], ' ', .)"/>
        </model><modelSequence predicate="parent::titleStmt/parent::fileDesc">
            <model predicate="preceding-sibling::title" behaviour="text">
                <param name="content" value="' — '"/>
            </model>
            <model behaviour="inline"/>
        </modelSequence><model predicate="not(@level) and parent::bibl" behaviour="inline"/><modelSequence predicate="@level='m' or not(@level)">
            <model behaviour="inline">
                <outputRendition xml:space="preserve">
                font-style: italic;
                </outputRendition>
            </model>
            <model predicate="ancestor::biblFull" behaviour="text">
                <param name="content" value="', '"/>
            </model>
        </modelSequence><modelSequence predicate="@level='s' or @level='j'">
            <model behaviour="inline">
                <outputRendition xml:space="preserve">
                font-style: italic;
                </outputRendition>
            </model>
            <model predicate="following-sibling::* and     (  ancestor::biblFull)" behaviour="text">
                <param name="content" value="', '"/>
            </model>
        </modelSequence><modelSequence predicate="@level='u' or @level='a'">
            <model behaviour="inline">
                <outputRendition xml:space="preserve">
                font-style: italic;
                </outputRendition>
            </model>
            <model predicate="following-sibling::* and     (    ancestor::biblFull)" behaviour="text">
                <param name="content" value="'. '"/>
            </model>
        </modelSequence><model behaviour="inline">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model></elementSpec>
                <elementSpec ident="author" mode="change"><model predicate="ancestor::teiHeader" behaviour="block">
            <outputRendition xml:space="preserve" scope="before">
            content: 'By ';
            </outputRendition>
        </model><model behaviour="inline"/></elementSpec>
                <elementSpec ident="edition" mode="change"><model predicate="ancestor::teiHeader" behaviour="block">
            <outputRendition xml:space="preserve" scope="before">
            content: 'Edition: ';
            </outputRendition>
        </model></elementSpec>
            <elementSpec ident="ref" mode="change"><model predicate="ancestor::div[@type=&#34;chapter&#34;]" behaviour="link">
            <param name="uri" value="@target"/>
            <outputRendition xml:space="preserve">
            color: #000; font-weight: bold;
            </outputRendition>
        </model><model predicate="ancestor::div[@type=&#34;intro&#34;]" behaviour="link">
            <param name="uri" value="@target"/>
            <outputRendition xml:space="preserve">
            color: #000; font-weight: 500;
            </outputRendition>
        </model><model predicate="not(@target)" behaviour="inline"/><model predicate="not(node())" behaviour="link">
            <param name="content" value="@target"/>
            <param name="uri" value="@target"/>
        </model><model behaviour="link">
            <param name="uri" value="@target"/>
        </model></elementSpec>
            <elementSpec ident="note" mode="change"><model output="web" predicate="@target" behaviour="note" cssClass="fn">
            <param name="label" value="."/>
            <param name="content" value="let $n := substring-after(@target, '#') return ./ancestor::TEI//div[@type='references']//note[@xml:id=$n]/node()"/>
            <outputRendition xml:space="preserve">
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            color: #E22717 !important; font-size: 80% !important; vertical-align: baseline !important;
position: relative !important; top: -0.4em !important;
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            </outputRendition>
        </model><model output="web" predicate="ancestor::teiHeader" behaviour="note">
            <param name="label" value="."/>
            <param name="content" value="let $n := substring-after(@target, '#') return ./ancestor::TEI//div[@type='references']//note[@xml:id=$n]/node()"/>
            <outputRendition xml:space="preserve">
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            color: #E22717 !important; font-size: 80% !important; vertical-align: baseline !important;
position: relative !important; top: -0.4em !important;
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            </outputRendition>
        </model><model predicate="parent::div[@type=&#34;references&#34;]" behaviour="block"/></elementSpec>
            <elementSpec ident="notatedMusic" mode="add"><model behaviour="figure">
            <param name="title" value="label"/>
            <param name="content" value="ptr"/>
        </model></elementSpec>
                <elementSpec ident="ptr" mode="add"><model predicate="parent::notatedMusic" behaviour="pass-through">
            <param name="url" value="@target"/>
            <pb:template xmlns="" xml:space="preserve"><pb-mei url="[[url]]" player="player">
  <pb-option name="appXPath" on="./rdg[contains(@label, 'original')]" off="">Original Clefs</pb-option>
</pb-mei></pb:template>
        </model></elementSpec>
            <elementSpec ident="formula" mode="change"><model predicate="@rendition='simple:display'" behaviour="block"/><model output="latex" predicate="@rend=&#34;display&#34;" behaviour="inline">
            <param name="content" value="string()"/>
            <pb:template xmlns="" xml:space="preserve">\begin{equation}[[content]]\end{equation}</pb:template>
        </model><model output="latex" behaviour="inline">
            <param name="content" value="string()"/>
            <pb:template xmlns="" xml:space="preserve">\begin{math}[[content]]\end{math}</pb:template>
        </model><model predicate="@rend='display'" behaviour="webcomponent">
            <param name="name" value="'pb-formula'"/>
            <param name="display" value="true()"/>
        </model><model behaviour="webcomponent">
            <param name="name" value="'pb-formula'"/>
        </model></elementSpec>
            <elementSpec ident="hi" mode="change"><model predicate="@rend=&#34;H&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            letter-spacing: 2px;
            </outputRendition>
        </model><model predicate="@rend=&#34;VNSUP&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            
            vertical-align: baseline !important;
position: relative !important; top: -0.4em !important; font-weight: bold; font-size: 110%; margin-right: 3px;
            
            </outputRendition>
        </model><model predicate="@rend=&#34;italics&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-style: italic !important;
            </outputRendition>
        </model><model predicate="@rend=&#34;italics bold&#34; or @rend=&#34;i b&#34; or @rend=&#34;bold italics&#34; or @rend=&#34;b i&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-weight: bold; font-style: italic;
