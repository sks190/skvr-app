        </model><model predicate="@rend=&#34;KA&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-weight: bold; font-style: normal;
            </outputRendition>
        </model><model predicate="@rend=&#34;I&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-style: italic; margin-right: 3px;
            </outputRendition>
        </model><model predicate="@rend=&#34;SUP&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            
            vertical-align: baseline;
position: relative;
top: -0.4em;
font-size: 85%;
            
            </outputRendition>
        </model><model predicate="@rend=&#34;SUB&#34;" behaviour="inline">
            <outputRendition xml:space="preserve">
            
            vertical-align: baseline;
position: relative;
bottom: -0.4em;
font-size: 85%;
            
            </outputRendition>
        </model><model predicate="@rendition" behaviour="inline" useSourceRendition="true">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model><model predicate="not(@rendition)" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model></elementSpec>
                <elementSpec ident="l" mode="change"><model predicate="@rend=&#34;CPT&#34;" behaviour="block">
            <outputRendition xml:space="preserve">
            font-size: 120%; margin-bottom: 1em;
            </outputRendition>
        </model><model predicate="ancestor::div[@type=&#34;chapter&#34;]" behaviour="block"/><model predicate="@rend=&#34;L&#34;" behaviour="block">
            <outputRendition xml:space="preserve">
            font-size: 85% !important;
            </outputRendition>
        </model><model predicate="@rend=&#34;K&#34;" behaviour="block">
            <outputRendition xml:space="preserve">
            font-size: 85% !important; font-style: italic !important;
            </outputRendition>
        </model><model predicate="@rend=&#34;TYHI&#34;" behaviour="block">
            <param name="content" value="&#34;tyhjä&#34;"/>
            <outputRendition xml:space="preserve">
            visibility: hidden;
            </outputRendition>
        </model><model behaviour="block"/><model behaviour="block" useSourceRendition="true">
            <outputRendition xml:space="preserve">
            margin-left: 1em;
            </outputRendition>
        </model></elementSpec>
                <elementSpec ident="catRef" mode="add"><model output="web" behaviour="link">
            <param name="content" value="let $id := substring-after(@target, '#')  return concat('-', doc('/db/apps/skvr/data/taksonomiataulu.xml')//category[@xml:id=$id]/catDesc[1])"/>
            <param name="uri" value="let $id := substring-after(@target, '#')  let $phrase := doc('/db/apps/skvr/data/taksonomiataulu.xml')//category[@xml:id=$id]/catDesc[1] return concat('/exist/apps/skvr/haku.html?query=&#34;', $phrase, '&#34;&amp;amp;coll=main&amp;amp;field=themes')"/>
            <param name="target" value="&#34;_blank&#34;"/>
            <outputRendition xml:space="preserve">
            font-size: 16px; margin-left: 0px; font-weight: 500; display: block; color: #000; text-decoration: none;
            </outputRendition>
        </model></elementSpec>
                <elementSpec ident="textClass" mode="add"><model behaviour="list">
            <param name="content" value="child::*"/>
            <outputRendition xml:space="preserve">
            margin-left: 15px; padding-left: 0px;
            </outputRendition>
        </model></elementSpec>
                <elementSpec ident="catDesc" mode="add"/>
                <elementSpec ident="category" mode="add"/>
                <elementSpec ident="taxonomy" mode="add"/>
