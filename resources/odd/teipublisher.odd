            </outputRendition>
        </model><model predicate="@type=&#34;intro&#34;" behaviour="block">
            <outputRendition xml:space="preserve">
            font-family: 'ff-scala-sans';
            </outputRendition>
        </model><model predicate="parent::body or parent::front or parent::back" behaviour="section"/><model behaviour="block"/></elementSpec>
                <elementSpec ident="p" mode="change"><model predicate="parent::div[@type=&#34;chapter&#34;]" behaviour="paragraph">
            <outputRendition xml:space="preserve">
            text-align: left; font-size: 15px; margin-bottom: 15px;
            </outputRendition>
        </model><model predicate="ancestor::list[@type=&#34;pair&#34;]" behaviour="paragraph">
